<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Detalle de incapacidad, filtro I</title>

  <link rel="stylesheet" href="/incapacidades/css/sidebar/style_sidebar_adaptacion_pantalla.css">



  <%- include('./../partials/header/header.ejs') %>
  <%-include('./../partials/listas ID/modulos/incapacidades/lista_cie_10.ejs') %>
</head>



<body class="bg-slate-200 ">


  <!-- NAVBAR -->
  <header>
    <!-- NAVBAR SUPERIOR -->
    <nav class="navbar">
      <div class="navbar-left ">
        <div class="navbar-left" style="gap: 0.5rem;">
          <img src="/incapacidades/img/logo/LOGO_SPEED_NETWORKS_SAS-01.png" alt="Logo 1" style="height: 60px;" />
          <img src="/incapacidades/img/logo/CONEXION-DIGITAL-LOGO-VECTOR-png-blanco.png" alt="Logo 2" style="height: 90px;" />
        </div>
      </div>

      <div class="navbar-right">
        <span class="navbar-user">Hola, Alex Alejandro Oviedo Gonzalez</span>
        <i class='bx bx-user-circle text-2xl ml-4'></i>
        <i class='bx bx-exit ml-4 text-xl cursor-pointer'></i>
      </div>
    </nav>
  </header>

  <!-- SIDEBAR -->
  <aside>
    <!-- SIDEBAR (partial) -->
    <div id="sidebar_principal_incapacidades" class="main-content">
      <aside id="sidebar" class="sidebar">
        <%- include('./../partials/sidebars/modulos/incapacidades/sidebar_principal.ejs') %>
      </aside>
    </div>

  </aside>


  <div class="w-full">

    <div class="flex w-full ">

      <div id="container_seleccionar_empleado" class="  min-h-screen gap-6 mt-32 mr-20 p-8 w-full max-w-11/12 transition-all duration-500 container-collapsed  bg-white rounded-xl">

       

        <div class="w-full mx-auto text-center border-b border-gray-400 pb-2 mb-4 mt-2 drop-shadow-md">
          <div class="grid grid-cols-1 mt-4">
            <h1 class="mt-2 ml-2 font-semibold font-serif text-sm text-start">
              TABLA LIQUIDACIÓN 
              <div class="w-full mx-auto text-center border-b border-gray-400 pb-1 mt-1 drop-shadow-md"></div>
            </h1>
        
            <div class="mt-8">
              <div class="relative overflow-x-auto shadow-md sm:rounded-lg">
                <table class="w-full text-xs text-left rtl:text-right text-gray-500">
                  <thead class="text-xs text-gray-100 bg-slate-800 py-0">
                    <tr>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">ACCESO</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">C. INCAPACIDAD</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">C. EMPLEADO</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">DOCUMENTO</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">T. INCAPACIDAD</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">ORIGEN</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">INICIO INCAPACIDAD</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">FINAL INCAPACIDAD</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">T. DÍAS</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">CATEGORIA</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">SUB-CATEGORIA</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">PRORROGA</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">DIAS EMPLEADOR</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">% EMPLEADOR</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">V. EMPLEADOR</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">DIAS EPS</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">% EPS</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">V. EPS</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">DIAS ARL</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">% ARL</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">V. ARL</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">DIAS F. PENSIONES</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">% F. PENSIONES</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">V. F. PENSIONES</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">DIAS EPS / F. PENSIONES</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">% EPS / F. PENSIONES</th>
                      <th scope="col" class="px-4 py-2 border border-white w-auto whitespace-nowrap">V. EPS / F. PENSIONES</th>
                    </tr>
                  </thead>
                  <tbody>


                    <% tabla_incapacidades.forEach(function(fila) { %>
                    
                        <tr class="odd:bg-slate-100 even:bg-gray-200 hover:bg-slate-300 border border-white">
                          <td class="px-4 py-0.5 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> 
                            <a href="/incapacidad/vista/liquidacion/entidades/<%= fila.id_incapacidades_liquidacion %>">
                              <i class='bx bxs-binoculars text-xl text-yellow-700'></i>
                            </a>
                          </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.id_incapacidades_liquidacion %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.id_empleado %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.documento %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.tipo_incapacidad %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap"> <%= fila.subtipo_incapacidad %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.fecha_inicio_incapacidad %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.fecha_final_incapacidad %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.cantidad_dias %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.codigo_categoria %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.codigo_subcategoria %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.prorroga === 1 ? "Sí" : "No" %></td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.dias_liquidacion_empleador %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.porcentaje_liquidacion_empleador %> %</td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= new Intl.NumberFormat('es-CO', { style: 'currency', currency: 'COP' }).format(fila.liquidacion_empleador) %> </td> 
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.dias_liquidacion_eps %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.porcentaje_liquidacion_eps %> %</td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-start"> <%= new Intl.NumberFormat('es-CO', { style: 'currency', currency: 'COP' }).format(fila.liquidacion_eps) %></td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.dias_liquidacion_arl %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.porcentaje_liquidacion_arl %> %</td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= new Intl.NumberFormat('es-CO', { style: 'currency', currency: 'COP' }).format(fila.liquidacion_arl) %></td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.dias_liquidacion_fondo_pensiones %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.porcentaje_liquidacion_fondo_pensiones %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= new Intl.NumberFormat('es-CO', { style: 'currency', currency: 'COP' }).format(fila.liquidacion_fondo_pensiones) %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.dias_liquidacion_eps_fondo_pensiones %> </td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= fila.porcentaje_liquidacion_eps_fondo_pensiones %> %</td>
                          <td class="px-4 py-1 text-xs font-medium text-slate-700 whitespace-nowrap text-center"> <%= new Intl.NumberFormat('es-CO', { style: 'currency', currency: 'COP' }).format(fila.liquidacion_eps_fondo_pensiones) %> </td>
                        </tr>

                    <% }); %>


                    
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
        
        
        

      </div>


    </div>

  </div>



  <!-- Script que maneja la sidebar y el contenedor -->
  <script src="/incapacidades/js/sidebar/sidebar_1.js"></script>

 


</body>

</html>



